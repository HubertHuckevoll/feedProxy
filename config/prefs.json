{
  "imagesSize": 384,
  "imagesTreatGIFs": true,
  "imagesAsJpeg": true,
  "encodingUTF8toAsciiAndEntities": true,
  "encodingUseOlderHTMLEntities": true,
  "outputLightOrDark": "light",
  "outputFontFace": "Arial",
  "overloadTreshold": 999,
  "downcycleTags": [
    "script", "style", "link",
    "picture", "video", "audio",
    "object", "embed", "template",
    "noscript", "aside", ".sidebar",
    "dialog",
    ".ad", ".ads", ".advertisement",
    ".ad-container", ".ad-banner", ".ad-unit",
    ".ad-slot", ".ad-wrapper", ".ad-section",
    ".ad-space", ".adbox", ".adsidebar",
    ".sponsored", ".sponsor", ".promo",
    ".promotional", ".commercial", ".advert",
    ".advertising", ".banner", ".ad-placeholder",
    ".advertisement-label", ".adsbygoogle"
  ],
  "downcycleAttrs": ["class", "style", "id"],
  "downcycleDynAttrs":  ["data-", "aria-", "x-"],
  "downcyclePutInHTML4Box": true,
  "downcycleDetectReaderable": true,
  "downcycleEnableForHTML4": false,
  "feedDetectionEnabled": true,
  "feedListLength": 15
}